{{#if products.length}}
<table>
    <tr>
        <th>Title</th>
        <th>Price</th>
        <th>Thumbnail</th>
    </tr>
    {{#each products}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img src={{this.thumbnail}} /></td>
        </tr>
    {{/each}}
</table>
{{/if}}